n({\n      email: email.value,\n      password: password.value,\n    })\n      .then((res) => {\n        email.value = \"\";\n        password.value = \"\";\n        TokenService.saveAuthToken(res.authToken);\n        const user = res.user;\n        localStorage.setItem(\"user email\", user.email);\n        localStorage.setItem(\"firstname\", user.firstname);\n        localStorage.setItem(\"lastname\", user.lastname);\n        this.props.history.push(\"/workouts\")\n        window.location.reload();\n      })\n      .catch((res) => {\n        console.log(res.message);\n        this.setState({ error: res.error });\n      });\n  };\n\n  render() {\n    return (\n      <div>\n        <header>Elevate</header>\n        <main className=\"wrapper\">\n          <div className=\"left-item\">\n            <img\n              src=\"https://jooinn.com/images/woman-exercising-4.jpg\"\n              alt=\"woman running\"\n            />\n          </div>\n          <div className=\"right-item\">\n            <form onSubmit={this.handleSubmitJwtAuth}>\n              {/*<label htmlFor=\"username\"> Username </label>*/}\n              <input\n                id=\"username\"\n                type=\"text\"\n                placeholder=\"Username\"\n                name=\"email\"\n              />\n              {/*<label htmlFor=\"password\"> Password </label>*/}\n              <input\n                id=\"password\"\n                type=\"password\"\n                placeholder=\"Password\"\n                name=\"password\"\n              />\n              <button id=\"login-btn\" type=\"submit\">\n                Log In\n              </button>\n              {this.state.error ? this.state.error : null}\n              <span>Forgot Password?</span>\n              <hr />\n              <button onClick={this.firstQuestion} type=\"button\">\n                New User\n              </button>\n            </form>\n          </div>\n        </main>\n        <div className=\"secondWrapper\">\n          <div>\n            <img\n              className=\"icon\"\n              src=\"https://img.icons8.com/fluent/96/000000/administrative-tools.png\"\n            />{\" \"}\n            <p>Customize your workouts </p>\n          </div>\n          <div>\n            <img\n              className=\"icon\"\n              src=\"https://img.icons8.com/cotton/64/000000/like--v4.png\"\n            />{\" \"}\n            <p>Exercise your mind, body and soul</p>\n          </div>\n          <div>\n            <img\n              className=\"icon\"\n              src=\"https://img.icons8.com/cotton/64/000000/checklist--v3.png\"\n            />{\" \"}\n            <p>Check your goals with a sustainable and effective plan</p>\n          </div>\n        </div>\n        <footer>Danielle Russell 2020</footer>\n      </div>\n    );\n  }\n}\n","/Users/russell/projects/elevate-app/src/components/name.js",[],"/Users/russell/projects/elevate-app/src/components/age.js",[],"/Users/russell/projects/elevate-app/src/components/weight.js",["89"],"import React from \"react\";\nimport \"../styles/weight.css\";\n\nexport default class Weight extends React.Component {\n \n  changeSlide = (e) => {\n    this.props.setWeight(e.target.value)\n  };\n\n  next = () => {\n    this.props.history.push(\"/height\");\n  };\n\n  back = () => {\n    this.props.history.goBack();\n  }\n\n  render() {\n    return (\n      <div>\n      <header> <span onClick={this.back}>&#8592;</span> Elevate </header>\n        <h1>Weight</h1>\n        <div className=\"age-range\">{this.props.weight == 400 ? this.props.weight + \"+\" : this.props.weight + \"lbs\"} </div>\n        <input\n          className=\"slider\"\n          onChange={this.changeSlide}\n          type=\"range\"\n          min=\"1\"\n          max=\"400\"\n        />\n        <button className=\"done\" onClick={this.next}>\n          Done!\n        </button>\n      </div>\n    );\n  }\n}\n","/Users/russell/projects/elevate-app/src/components/height.js",[],"/Users/russell/projects/elevate-app/src/components/goals.js",[],"/Users/russell/projects/elevate-app/src/components/time.js",[],"/Users/russell/projects/elevate-app/src/components/summary.js",["90","91","92"],"import React from \"react\";\nimport \"../styles/summary.css\";\nimport AuthApiService from \"../auth-api-service\";\nimport config from \"../config\";\n\nexport default class Summary extends React.Component {\n  state = {\n    hasError: false,\n  };\n  array = () => {\n    for (let i = 0; i < this.props.state.goals.length; i++) {\n      return this.props.state.goals[i][0].name;\n    }\n  };\n\n  determineIcon = () => {\n    if (this.array() === \"Tone Up\") {\n      return <img src=\"https://img.icons8.com/color/48/000000/pilates.png\" />;\n    } else if (this.array() === \"Lose Weight\") {\n      return <img src=\"https://img.icons8.com/dusk/64/000000/scale.png\" />;\n    } else if (this.array() === \"Improve Fitness\") {\n      return <img src=\"https://img.icons8.com/cotton/64/000000/like--v3.png\" />;\n    }\n  };\n  postPref = () => {\n    let id = localStorage.getItem(\"user email\");\n    let preferences = {\n      userid: id,\n      name: this.props.state.name,\n      age: 0,\n      weight: parseInt(this.props.state.weight),\n      height: this.props.state.height,\n      goals: this.props.state.goals[0][0].name,\n      time:\n        this.props.state.time[0][0].name +\n        \",\" +\n        this.props.state.time[0][1].name,\n      days:\n        this.props.state.days[0][0].name +\n        \",\" +\n        this.props.state.days[0][1].name +\n        \",\" +\n        this.props.state.days[0][2].name +\n        \",\" +\n        this.props.state.days[0][3].name,\n    };\n    fetch(`${config.API_ENDPOINT}/api/preferences`, {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(preferences),\n    })\n      .then((response) => {\n        return response.json();\n      })\n      .then((data) => this.props.addPref(data))\n      .catch((error) => {\n        this.setState({ hasError: true });\n      });\n  };\n\n  handleNewUser = (ev) => {\n    ev.preventDefault();\n    const { firstname, lastname, email, password } = ev.target;\n\n    AuthApiService.postUser({\n      firstname: firstname.value,\n      lastname: lastname.value,\n      email: email.value,\n      password: password.value,\n    })\n      .then((user) => {\n        this.setState({ error: false });\n        localStorage.setItem(\"user email\", email.value);\n        localStorage.setItem(\"firstname\", firstname.value);\n        localStorage.setItem(\"lastname\", lastname.value);\n        AuthApiService.postLogin({\n          email: email.value,\n          password: password.value,\n        });\n      })\n      .catch((res) => {\n        this.setState({ error: res.error });\n      });\n  };\n\n  next = () => {\n    this.props.history.push(\"/workouts\")\n      }\n  render() {\n    return (\n      <div>\n        <div className=\"goal-con\">\n          <h1>Goal </h1>\n          <br /> {this.determineIcon()} <br /> <p>{this.array()}</p>\n        </div>\n        <p>\n          {this.props.state.name}, based on the information you have given us,\n          we have determined a personal workout plan that we believe is best\n          suited to you. Follow the directions to customize your workout plans.\n          Happy working out!\n        </p>\n        <div className=\"form-wrap\">\n          <form id=\"landing-form\" onSubmit={this.handleNewUser}>\n            <b className=\"log\"> Create Account</b>\n            <br />\n            <br />\n            <label htmlFor=\"firstname\">First Name: </label>\n            <input\n              id=\"firstname\"\n              name=\"firstname\"\n              type=\"text\"\n              placeholder=\"First Name\"\n              required\n            />\n            <label htmlFor=\"lastname\">Last Name: </label>\n            <input\n              id=\"lastname\"\n              name=\"lastname\"\n              type=\"text\"\n              placeholder=\"Last Name\"\n              required\n            />\n            <label htmlFor=\"email\">Email: </label>\n            <input\n              id=\"email\"\n              name=\"email\"\n              type=\"email\"\n              placeholder=\"email\"\n              required\n            />\n            <label htmlFor=\"password\">Password (8 Characters): </label>\n            <input\n              id=\"password\"\n              name=\"password\"\n              type=\"password\"\n              placeholder=\"password\"\n              required\n            />\n            <button onClick={this.postPref} className=\"new\" type=\"submit\">\n              Submit\n            </button>\n            <button className=\"done\" onClick={this.next}>See Plans</button>\n           \n          </form>\n        </div>\n      </div>\n    );\n  }\n}\n","/Users/russell/projects/elevate-app/src/components/workouts.js",["93","94"],"/Users/russell/projects/elevate-app/src/components/modal.js",[],"/Users/russell/projects/elevate-app/src/components/secondModal.js",[],"/Users/russell/projects/elevate-app/src/components/days.js",["95"],"import React from \"react\";\nimport config from \"../config\"\n\nexport default class Days extends React.Component {\n  state = {\n    daysArr: [\n      { name: \"Mon\", isActive: false },\n      { name: \"Tues\", isActive: false },\n      { name: \"Weds\", isActive: false },\n      { name: \"Thurs\", isActive: false },\n      { name: \"Fri\", isActive: false },\n      { name: \"Sat\", isActive: false },\n      { name: \"Sun\", isActive: false },\n    ],\n  };\n\n  next = () => {\n    let days = this.state.daysArr.filter((day) => day.isActive);\n    this.props.setDays(days);\n    this.props.history.push(\"/summary\")\n    };\n  \n\n  onClick(index) {\n    let tmp = this.state.daysArr;\n    tmp[index].isActive = !tmp[index].isActive;\n    this.setState({ daysArr: tmp });\n  }\n\n  back = () => {\n    this.props.history.goBack();\n  };\n\n  active = () => {\n    let days = this.state.daysArr.filter((day) => day.isActive);\n    if (days.length !== 4) {\n      return true;\n    }\n  };\n\n  render() {\n    return (\n      <div>\n        <header>\n          <span onClick={this.back}>&#8592;</span> Elevate\n        </header>\n        <h1>What Days Can You Workout?</h1>\n        <div>\n          {this.state.daysArr.map((el, index) => (\n            <div\n              className={el.isActive ? \"active\" : \"inactive\"}\n              key={index}\n              onClick={() => this.onClick(index)}\n            >\n              {el.name}\n            </div>\n          ))}\n        </div>\n        {this.active() ? \"Please select 4\" : null}\n        <button onClick={this.next} disabled={this.active()} className=\"done\">\n          Done\n        </button>\n      </div>\n    );\n  }\n}\n","/Users/russell/projects/elevate-app/src/components/dummyData.js",[],"/Users/russell/projects/elevate-app/src/config.js",["96"],"export default {\n    API_ENDPOINT: `https://tranquil-fortress-76141.herokuapp.com`,\n    TOKEN_KEY: \"elevate-auth-token\",\n}","/Users/russell/projects/elevate-app/src/auth-api-service.js",[],"/Users/russell/projects/elevate-app/src/tokenService.js",[],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":80,"column":13,"nodeType":"103","endLine":83,"endColumn":15},{"ruleId":"101","severity":1,"message":"102","line":87,"column":13,"nodeType":"103","endLine":90,"endColumn":15},{"ruleId":"101","severity":1,"message":"102","line":94,"column":13,"nodeType":"103","endLine":97,"endColumn":15},{"ruleId":"104","severity":1,"message":"105","line":23,"column":55,"nodeType":"106","messageId":"107","endLine":23,"endColumn":57},{"ruleId":"101","severity":1,"message":"102","line":18,"column":14,"nodeType":"103","endLine":18,"endColumn":78},{"ruleId